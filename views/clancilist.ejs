<!DOCTYPE html>
<html dir="ltr" lang="en-US">
<head>
    <!-- Document Meta
        ============================================= -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <!--IE Compatibility Meta-->
    <meta name="author" content="zytheme"/>
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <meta name="description" content="Multi-purpose Business html5 template">
    <link href="assets/images/favicon/favicon.png" rel="icon">

    <!-- Fonts
        ============================================= -->
    <link href="http://fonts.googleapis.com/css?family=Raleway:100,100i,200,200i,300,300i,400,400i,500,500i,600,600i,700,700i,800,800i,900,900i%7COpen+Sans:300,300i,400,400i,600,600i,700,700i,800,800i%7CPoppins:300,400,500,600,700" rel="stylesheet" type="text/css">

    <!-- Stylesheets
        ============================================= -->
    <link href="assets/css/external.css" rel="stylesheet">
    <link href="assets/css/bootstrap.min.css" rel="stylesheet">
    <link href="assets/css/style.css" rel="stylesheet">

    <!-- HTML5 shim, for IE6-8 support of HTML5 elements. All other JS at the end of file. -->
    <!--[if lt IE 9]>
    <script src="assets/js/html5shiv.js"></script>
    <script src="assets/js/respond.min.js"></script>
    <![endif]-->

    <!-- Document Title
        ============================================= -->
    <title>Rene Vegan</title>
</head>
<body>
<div class="preloader">
    <div class="load-bar">
        <div class="bar"></div>
        <div class="bar"></div>
        <div class="bar"></div>
    </div>
</div><!-- Document Wrapper
	============================================= -->
<div id="wrapper" class="wrapper clearfix">
    <% include partials/header.ejs %>

    <!-- Page Title #10
    ============================================= -->
    <section id="page-title" class="page-title bg-overlay bg-overlay-dark5 bg-parallax">
        <div class="bg-section">
            <img src="/images/paralax/meksicka-salata.jpg" alt="Background"/>
        </div>
        <div class="container">
            <div class="row">
                <div class="col-xs-12 col-sm-12 col-md-8 col-md-offset-2">
                    <div class="title title-10 text-center">

                        <div class="title--heading">
                            <h1 class="text--uppercase">Članci</h1>
                        </div>
                        <div class="title--desc">
                            <p>Možete pročitati sve što niste znali do sad a zanima vas</p>
                        </div>
                    </div><!-- .title end -->
                </div><!-- .col-md-8 end -->
            </div><!-- .row end -->
        </div><!-- .container end -->
    </section><!-- #page-title end -->

    <!-- Blog Grid
    ============================================= -->
    <section id="blog" class="blog blog-grid">
        <div class="container">
            <div class="row">
                <!-- Blog Entry #1 -->
                <% if(data){ %>
                <% data.forEach(function (clanak) { %>
                <div class="col-xs-12 col-sm-6 col-md-4">
                    <div class="blog-entry">
                        <div class="entry--img" >
                            <a href="/clanci/<%= clanak.id %>">
                                <img src="/images/<%= clanak.filename %>" alt="<%= clanak.filename %>" style="height: 182px; width: 100%;"/>
                            </a>
                        </div>
                        <div class="entry--content" >
                            <div class="entry--meta">
                                <span class="entry--meta-date"><%= moment(clanak.created_on).format("Do MMM YYYY");%></span>
                            </div>
                            <div class="entry--title">
                                <h4><a href=/clanci/<%= clanak.id %>><%= clanak.title%></a></h4>
                            </div>
                            <div class="entry--bio">
                                <%- clanak.description %>
                            </div>
                            <div class="entry--footer">
                                <div class="entry--by">
                                    <img src="/assets/images/blog/author/1.png" alt="entry author">
                                    <div class="entry--by-content">
                                        by : <a href="#"><%= clanak.username %></a>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- .entry-content end -->
                    </div>
                    <!-- .blog-entry end -->
                </div>
                <!-- .col-md-4 end -->
                <%  });} %>

            </div>
            <!-- .row end -->
            <div class="row">
                <div class="col-xs-12 col-sm-12 col-md-12 clearfix mt-20 text--center">
                    <ul class="pagination">
                        <li class="active"><a href="#">1</a>
                        </li>
                        <li><a href="#">2</a>
                        </li>
                        <li><a href="#">3</a>
                        </li>
                        <li>
                            <a href="#" aria-label="Next">
                                <span aria-hidden="true"><i class="fa fa-angle-right"></i></span>
                            </a>
                        </li>
                    </ul>
                </div>
                <!-- .col-md-12 end -->
            </div>
            <!-- .row end -->
        </div>
        <!-- .container end -->
    </section>
    <!-- #blog end -->

    <!-- Footer #1
    ============================================= -->
    <footer id="footer" class="footer footer-1">
        <!-- Widget Section
        ============================================= -->
        <div class="footer-widget">
            <div class="container">
                <div class="row">
                    <div class="col-xs-12 col-sm-4 col-md-3 footer--widget footer--widget-about">
                        <div class="footer--widget-title">
                            <h5>About Us</h5>
                        </div>
                        <div class="footer--widget-content">
                            <p>Our brand promise is simple: to provide powerful digital marketing solutions to small businesses.</p>
                            <ul class="list-unstyled">
                                <li><span>Call US:</span> 002 01065370701</li>
                                <li><span>Email US:</span> ayman@zytheme.com</li>
                            </ul>
                            <div class="footer-social-icon">
                                <a class="facebook" href="#">
                                    <i class="fa fa-facebook"></i><i class="fa fa-facebook"></i>
                                </a>
                                <a class="twitter" href="#" >
                                    <i class="fa fa-twitter"></i><i class="fa fa-twitter"></i>
                                </a>
                                <a class="instagram" href="#">
                                    <i class="fa fa-instagram"></i><i class="fa fa-instagram"></i>
                                </a>
                                <a class="linkedin" href="#">
                                    <i class="fa fa-linkedin"></i><i class="fa fa-linkedin"></i>
                                </a>
                            </div>
                        </div>
                    </div><!-- .col-md-3 end -->
                    <div class="col-xs-12 col-sm-4 col-md-3 footer--widget footer--widget-recent">
                        <div class="footer--widget-title">
                            <h5>Recent Posts</h5>
                        </div>
                        <div class="footer--widget-content">
                            <div class="entry">
                                <div class="entry--img">
                                    <a href="#">
                                        <img src="assets/images/footer/thumb-1.jpg" alt="entry image">
                                    </a>
                                </div>
                                <div class="entry--content">
                                    <div class="entry--title">
                                        <a href="#">Four ways to cheer up on Blue Monday!</a>
                                    </div>
                                    <div class="entry--meta">Feb 27, 2018</div>
                                </div>
                            </div><!-- .entry end -->
                            <div class="entry">
                                <div class="entry--img">
                                    <a href="#">
                                        <img src="assets/images/footer/thumb-2.jpg" alt="entry image">
                                    </a>
                                </div>
                                <div class="entry--content">
                                    <div class="entry--title">
                                        <a href="#">Old cameras can capture images better</a>
                                    </div>
                                    <div class="entry--meta">Feb 27, 2018</div>
                                </div>
                            </div><!-- .entry end -->
                        </div>
                    </div><!-- .col-md-3 end -->
                    <div class="col-xs-12 col-sm-4 col-md-3 footer--widget footer-widget-tweets tweets-widget">
                        <div class="footer--widget-title">
                            <h5>Latest Tweets</h5>
                        </div>
                        <div class="footer-widget-content">
                            <div id="twitter-feed" data-numtweets="2"></div>
                        </div>
                    </div><!-- .col-md-3 end -->
                    <div class="col-xs-12 col-sm-12 col-md-3 footer--widget widget-instagram">
                        <div class="footer--widget-title">
                            <h5>Instagram</h5>
                        </div>
                        <div class="footer--widget-content">
                            <div id="instafeedFooter" class="instafeed" data-insta-number="8"></div>
                        </div>
                    </div><!-- .col-md-3 end -->
                </div>
            </div><!-- .container end -->
        </div><!-- .footer-widget end -->

        <!-- Copyrights
        ============================================= -->
        <div class="footer--bar">
            <div class="container">
                <div class="row">
                    <div class="col-xs-12 col-sm-12 col-md-12 text--center">
                        <div class="footer--copyright">
                            <span>&copy; 2018 Enormous, With Love by</span> <a href="http://themeforest.net/user/7oroof/portfolio?ref=zytheme">7oroof.com</a>
                        </div>
                    </div>

                </div>
            </div><!-- .container end -->
        </div><!-- .footer-copyright end -->
    </footer>
</div><!-- #wrapper end -->

<!-- Footer Scripts
============================================= -->
<script src="assets/js/jquery-2.2.4.min.js"></script>
<script src="assets/js/plugins.js"></script>
<script src="assets/js/functions.js"></script>
</body>
</html>